<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- mybatis가 NULL값을 인식하도록 하는 설정 -->
		<setting name="jdbcTypeForNull" value="VARCHAR" />
		<setting name="jdbcTypeForNull" value="DATE" />

		<!-- camel표기법 처리 -->
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>

	<typeAliases>  <!-- 수정요망 -->

		<typeAlias type="com.lifemenu.dto.CoinRepositoryDto" alias="coinRepositoryDto"/>
		<typeAlias type="com.lifemenu.dto.WalletDto" alias="walletDto"/>
		<typeAlias type="com.lifemenu.dto.WalletDtlsDto" alias="walletDtlsDto"/>


		

	</typeAliases>

</configuration>
